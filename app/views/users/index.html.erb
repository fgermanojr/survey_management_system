<h1>Users#index</h1>
<!-- app/views/users/index.html.erb -->

<%= link_to 'New User', new_user_path %>
<br>
<br>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>email</th>
      <th>isAdmin</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.is_admin %></td>
      </tr>
    <% end %>
  </tbody>
</table>
