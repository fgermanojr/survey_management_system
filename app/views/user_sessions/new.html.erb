<h1>Login</h1>
<%= form_with model: @user, url: user_sessions_create_path, method: :get do |f| %>
  <div>
    <%= f.label :email%><br>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <p>
    <%= f.submit 'Login' %>
  </p>
<% end %>
